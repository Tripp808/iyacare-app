# Firebase Configuration
# Get these values from your Firebase project settings
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# External AI Model Configuration (Optional)
# Set these to integrate with your AI classification model
NEXT_PUBLIC_AI_MODEL_URL=https://your-ai-model.onrender.com
NEXT_PUBLIC_AI_MODEL_API_KEY=your_ai_model_api_key_here

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: For analytics and other features
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Twilio Configuration (SMS Service)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# App Configuration
NEXT_PUBLIC_APP_NAME=Iy√†Care

# Blockchain Configuration (Ethereum Sepolia Testnet)
# Get these from Alchemy/Infura and your MetaMask wallet
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/your_api_key
PRIVATE_KEY=your_metamask_private_key_for_deployment
ETHERSCAN_API_KEY=your_etherscan_api_key_for_verification

# Contract Addresses (Will be set after deployment)
NEXT_PUBLIC_SEPOLIA_CONTRACT_ADDRESS=your_deployed_contract_address
NEXT_PUBLIC_NETWORK_ID=11155111

# Blockchain Settings
NEXT_PUBLIC_ENABLE_BLOCKCHAIN=true
NEXT_PUBLIC_BLOCKCHAIN_NETWORK=sepolia 